      <input ref="/Sample/basic/select_multiple_other_1_other">
        <label>Specify other.</label>
      </input>
      <input ref="/Sample/basic/date_1">
        <label>Date</label>
        <hint>Hint text goes here:</hint>
      </input>
      <input ref="/Sample/basic/time_1">
        <label>Time</label>
        <hint>Hint text goes here:</hint>
      </input>
      <upload mediatype="image/*" ref="/Sample/basic/image_1">
        <label>Image</label>
        <hint>Hint text goes here:</hint>
      </upload>
      <input ref="/Sample/basic/geopoint_1">
        <label>Geopoint</label>
        <hint>Hint text goes here:</hint>
      </input>
      <input ref="/Sample/basic/note">
        <label>Note</label>
        <hint>This field is for display on the form only.</hint>
      </input>
    </group>
    <group ref="/Sample/other">
      <label>Other Types:</label>
      <input ref="/Sample/other/date_required">
        <label>Required</label>
        <hint>This field is required</hint>
      </input>
      <input ref="/Sample/other/text_default">
        <label>Default</label>
        <hint>This is a default value</hint>
      </input>
      <input ref="/Sample/other/int_readonly">
        <label>Read Only</label>
        <hint>This field is read only</hint>
      </input>
      <input ref="/Sample/other/int_calc">
        <label>Calculated Field</label>
        <hint>You can recalculate this field after editing by hitting refresh</hint>
      </input>
    </group>
    <group ref="/Sample/group_2">
      <label>Constraints</label>
      <input ref="/Sample/group_2/cons_text">
        <label>Text Constraint</label>
        <hint>Enter your email address</hint>
      </input>
      <input ref="/Sample/group_2/cons_integer">
        <label>Integer Constraint</label>
        <hint>Only numbers &gt; 100</hint>
      </input>
      <input ref="/Sample/group_2/cons_decimal">
        <label>Decimal Constraint</label>
        <hint>Only numbers &gt; 10.51 and &lt; 18.39</hint>
      </input>
    </group>
    <group ref="/Sample/group_3">
      <label>Relevant</label>
      <select1 ref="/Sample/group_3/select_relevant">
