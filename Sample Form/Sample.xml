      <bind nodeset="/Sample/basic/text_1" type="string"/>
      <bind nodeset="/Sample/basic/integer_1" type="int"/>
      <bind nodeset="/Sample/basic/decimal_1" type="decimal"/>
      <bind nodeset="/Sample/basic/select_one_1" type="select1"/>
      <bind nodeset="/Sample/basic/select_one_otheer_1" type="select1"/>
      <bind nodeset="/Sample/basic/select_one_otheer_1_other" relevant="selected(../select_one_otheer_1, 'other')" type="string"/>
      <bind nodeset="/Sample/basic/select_multiple_1" type="select"/>
      <bind nodeset="/Sample/basic/select_multiple_other_1" type="select"/>
      <bind nodeset="/Sample/basic/select_multiple_other_1_other" relevant="selected(../select_multiple_other_1, 'other')" type="string"/>
      <bind nodeset="/Sample/basic/date_1" type="date"/>
      <bind nodeset="/Sample/basic/time_1" type="time"/>
      <bind nodeset="/Sample/basic/image_1" type="binary"/>
      <bind nodeset="/Sample/basic/geopoint_1" type="geopoint"/>
      <bind nodeset="/Sample/basic/note" readonly="true()" type="string"/>
      <bind nodeset="/Sample/other/date_required" required="true()" type="date"/>
      <bind nodeset="/Sample/other/text_default" type="string"/>
      <bind nodeset="/Sample/other/int_readonly" readonly="true()" type="int"/>
      <bind calculate="100 *  /Sample/other/int_readonly " nodeset="/Sample/other/int_calc" type="int"/>
      <bind constraint="regex(., '[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,4}')" jr:constraintMsg="Enter a valid email address e.g., name@email.com" nodeset="/Sample/group_2/cons_text" type="string"/>
      <bind constraint=". &gt;=100" jr:constraintMsg="Number must be larger than 100" nodeset="/Sample/group_2/cons_integer" type="int"/>
      <bind constraint=". &gt; 10.51 and . &lt; 18.39" jr:constraintMsg="Requires a number between 10.51 and 18.39" nodeset="/Sample/group_2/cons_decimal" type="decimal"/>
      <bind nodeset="/Sample/group_3/select_relevant" type="select1"/>
      <bind nodeset="/Sample/group_3/note_relvevant" readonly="true()" relevant=" /Sample/group_3/select_relevant  = 1" type="string"/>
      <bind nodeset="/Sample/group_3/note_irrelevant" readonly="true()" relevant=" /Sample/group_3/select_relevant  = 0" type="string"/>
      <bind nodeset="/Sample/group_3/select_multiple_relevant" type="select"/>
      <bind nodeset="/Sample/group_3/text_relevant" relevant="selected( /Sample/group_3/select_multiple_relevant , 'blue')" type="string"/>
      <bind nodeset="/Sample/group_3/skip_group" type="select1"/>
      <bind nodeset="/Sample/group_3/skip" relevant=" /Sample/group_3/skip_group  = 0"/>
      <bind nodeset="/Sample/group_3/skip/note_1" readonly="true()" type="string"/>
      <bind nodeset="/Sample/language/note_lang" readonly="true()" type="string"/>
      <bind nodeset="/Sample/language/select_animal" type="select1"/>
      <bind nodeset="/Sample/appearance/text_multiline" type="string"/>
      <bind nodeset="/Sample/appearance/select_one_no_style" type="select1"/>
      <bind nodeset="/Sample/appearance/select_minimal_one" type="select1"/>
      <bind nodeset="/Sample/appearance/select_horizontal_one" type="select1"/>
      <bind nodeset="/Sample/appearance/select_one_shapes" type="select1"/>
      <bind nodeset="/Sample/appearance/select_compact_one" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Sample/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/Sample/basic">
      <label>Basic Types:</label>
      <input ref="/Sample/basic/text_1">
        <label>Text</label>
        <hint>Hint text goes here:</hint>
      </input>
      <input ref="/Sample/basic/integer_1">
        <label>Integer</label>
        <hint>Hint text goes here:</hint>
      </input>
      <input ref="/Sample/basic/decimal_1">
        <label>Decimal</label>
        <hint>Hint text goes here:</hint>
      </input>
      <select1 ref="/Sample/basic/select_one_1">
        <label>Select One</label>
        <hint>Hint text goes here:</hint>
        <item>
          <label>no</label>
          <value>0</value>
        </item>
        <item>
          <label>yes</label>
          <value>1</value>
        </item>
      </select1>
      <select1 ref="/Sample/basic/select_one_otheer_1">
        <label>Select One or Other</label>
        <hint>Hint text goes here:</hint>
        <item>
          <label>no</label>
          <value>0</value>
        </item>
        <item>
          <label>yes</label>
          <value>1</value>
        </item>
        <item>
          <label>Other</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/Sample/basic/select_one_otheer_1_other">
        <label>Specify other.</label>
      </input>
      <select ref="/Sample/basic/select_multiple_1">
        <label>Select Multiple</label>
        <hint>Hint text goes here:</hint>
        <item>
          <label>Blue</label>
          <value>blue</value>
        </item>
        <item>
          <label>Red</label>
          <value>red</value>
