        </item>
        <item>
          <label>yes</label>
          <value>1</value>
        </item>
      </select1>
      <group ref="/Sample/group_3/skip">
        <label>Lions</label>
        <input ref="/Sample/group_3/skip/note_1">
          <label ref="jr:itext('/Sample/group_3/skip/note_1:label')"/>
        </input>
      </group>
    </group>
    <group ref="/Sample/language">
      <label>Multiple Languages:</label>
