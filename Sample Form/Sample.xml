        <Sample id="myform">
          <basic>
            <text_1/>
            <integer_1/>
            <decimal_1/>
            <select_one_1/>
            <select_one_otheer_1/>
            <select_one_otheer_1_other/>
            <select_multiple_1/>
            <select_multiple_other_1/>
            <select_multiple_other_1_other/>
            <date_1/>
            <time_1/>
            <image_1/>
            <geopoint_1/>
            <note/>
          </basic>
          <other>
            <date_required/>
            <text_default>Default Text</text_default>
            <int_readonly>100</int_readonly>
            <int_calc/>
          </other>
          <group_2>
            <cons_text/>
            <cons_integer/>
            <cons_decimal/>
          </group_2>
          <group_3>
            <select_relevant/>
            <note_relvevant/>
            <note_irrelevant/>
            <select_multiple_relevant/>
            <text_relevant/>
            <skip_group/>
            <skip>
              <note_1/>
            </skip>
          </group_3>
          <language>
            <note_lang/>
            <select_animal/>
          </language>
          <appearance>
            <text_multiline/>
            <select_one_no_style/>
            <select_minimal_one/>
            <select_horizontal_one/>
            <select_one_shapes/>
            <select_compact_one/>
          </appearance>
          <meta>
            <instanceID/>
          </meta>
        </Sample>
